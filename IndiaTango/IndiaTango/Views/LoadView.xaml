<Window x:Class="IndiaTango.Views.LoadView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadView" Height="164" Width="155" ShowInTaskbar="False" WindowStyle="ToolWindow">
    <Grid Height="118" Width="133">
        <ProgressBar Height="25" HorizontalAlignment="Left" Margin="12,38,0,0" Name="progressBar1" VerticalAlignment="Top" Width="104" Value="{Binding ProgressValue}" />
        <Label Content="Loading file..." Height="29" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" Width="78" FontSize="12" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="11,73,0,0" Name="BtnCancel" VerticalAlignment="Top" Width="78" />
    </Grid>
</Window>
